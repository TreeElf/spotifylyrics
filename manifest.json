{
  "name": "Spotify Lyrics",
  "description": "Finds the lyrics for the current spotify song",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {                    
        "16": "icon.png",          
        "24": "icon.png",         
        "32": "icon.png"
    }
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}